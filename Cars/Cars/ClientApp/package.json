    "@angular/common": "~12.2.2",
    "@angular/compiler": "~12.2.2",
    "@angular/core": "~12.2.2",
    "@angular/forms": "~12.2.2",
    "@angular/material": "~12.2.2",
    "@angular/platform-browser": "~12.2.2",
    "@angular/platform-browser-dynamic": "~12.2.2",
    "@angular/platform-server": "~12.2.2",
    "@angular/router": "~12.2.2",
    "@auth0/angular-jwt": "^5.0.2",
    "@geoapify/angular-geocoder-autocomplete": "^1.3.3",
    "@geoapify/geocoder-autocomplete": "^1.3.2",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "algoliasearch": "^3.35.1",
    "bootstrap": "^5.1.0",
    "jquery": "^3.5.1",
